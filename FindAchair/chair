#include<bits/stdc++.h>
using namespace std;
int main(){
    int count=0;
    int diff[255];
    for(int i=0; i<arr.length(); i++){
        if(count==8){
            break;
        }
        if(arr[0][1].length())=arr[i][1]){
            count += abs((arr[0][1].length) - arr[i][1])
            diff[i] = count;
        }
        return diff;
    }
}
